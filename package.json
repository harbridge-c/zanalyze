{
    "name": "@vortiq/zanalyze",
    "version": "0.0.1",
    "description": "EML Processor",
    "main": "dist/main.js",
    "type": "module",
    "bin": {
        "zanalyze": "./dist/main.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vortiq/zanalyze.git"
    },
    "packageManager": "pnpm@10.10.0",
    "scripts": {
        "build": "tsc --noEmit && vite build && copyfiles -u 1 \"src/**/*.md\" dist && copyfiles -u 1 \"src/**/*.yaml\" dist",
        "start": "node dist/main.js",
        "watch": "vite build --watch",
        "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run clean && npm run build"
    },
    "keywords": [
        "data",
        "simplification",
        "filter"
    ],
    "author": "Colton Harbridge <harbridgecolton@hotmail.com>",
    "license": "Apache-2.0",
    "dependencies": {
        "@maxdrellin/xenocline": "^0.0.2",
        "@riotprompt/riotprompt": "^0.0.1",
        "@theunwalked/cardigantime": "^0.0.1",
        "@theunwalked/dreadcabinet": "^0.0.1",
        "@vortiq/eml-parse-js": "^0.0.1",
        "commander": "^14.0.0",
        "dayjs": "^1.11.13",
        "deepmerge-ts": "^7.1.5",
        "dotenv": "^16.5.0",
        "eml-parse-js": "1.2.0-beta.0",
        "glob": "^11.0.2",
        "js-yaml": "^4.1.0",
        "luxon": "^3.6.1",
        "moment-timezone": "^0.5.48",
        "openai": "4.98.0",
        "winston": "^3.17.0",
        "zod": "3.24.4"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.27.0",
        "@jest/globals": "^29.7.0",
        "@rollup/plugin-replace": "^6.0.2",
        "@swc/core": "^1.11.24",
        "@types/jest": "^29.5.14",
        "@types/js-yaml": "^4.0.9",
        "@types/luxon": "^3.6.2",
        "@types/node": "^22.15.19",
        "@typescript-eslint/eslint-plugin": "^8.32.1",
        "@typescript-eslint/parser": "^8.32.1",
        "concurrently": "^9.1.2",
        "copyfiles": "^2.4.1",
        "eslint": "^9.27.0",
        "eslint-plugin-import": "^2.31.0",
        "globals": "^16.1.0",
        "jest": "^29.7.0",
        "mockdate": "^3.0.5",
        "rollup-plugin-preserve-shebang": "^1.0.1",
        "rollup-plugin-visualizer": "^5.14.0",
        "ts-jest": "^29.3.4",
        "typescript": "^5.8.3",
        "vite": "^6.3.5",
        "vite-plugin-node": "^5.0.1"
    }
}